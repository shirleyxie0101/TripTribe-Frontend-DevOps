{
  "name": "triptribe-frontend",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "npm run gql:compile && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --fix",
    "lint:fix": "eslint --fix .",
    "prettier": "prettier --write .",
    "format": "npm run lint:fix && npm run prettier",
    "prepare": "husky install",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "gql:compile": "graphql-codegen",
    "gql:watch": "graphql-codegen -w"
  },
  "dependencies": {
    "@apollo/client": "^3.8.9",
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.1",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@hookform/resolvers": "^3.3.2",
    "@mui/icons-material": "^5.14.15",
    "@mui/material": "^5.14.13",
    "@mui/system": "^5.14.13",
    "@sentry/nextjs": "^7.91.0",
    "@types/mapbox-gl": "^2.7.18",
    "@types/tz-lookup": "^6.1.2",
    "@types/yup": "^0.32.0",
    "@untitled-ui/icons-react": "^0.1.2",
    "axios": "^1.6.0",
    "dangerously-set-html-content": "^1.1.0",
    "date-fns": "^3.6.0",
    "dayjs": "^1.11.10",
    "deep-chat-react": "^1.4.11",
    "graphql": "^16.8.1",
    "lodash.isequal": "^4.5.0",
    "mapbox-gl": "^2.15.0",
    "next": "13.5.4",
    "notistack": "^3.0.1",
    "photoswipe": "^5.4.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-dropzone": "^14.2.3",
    "react-helmet-async": "^2.0.4",
    "react-hook-form": "^7.47.0",
    "react-hot-toast": "^2.4.1",
    "react-intersection-observer": "^9.5.3",
    "react-map-gl": "^7.1.6",
    "react-quill": "^2.0.0",
    "simplebar-react": "^3.2.4",
    "stylis-plugin-rtl": "^2.1.1",
    "swr": "^2.2.4",
    "tailwindcss": "3.3.3",
    "tz-lookup": "^6.1.25",
    "yup": "^1.3.2",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.10",
    "@types/lodash.isequal": "^4.5.8",
    "@types/node": "20.8.2",
    "@types/react": "18.2.25",
    "@types/react-dom": "18.2.10",
    "autoprefixer": "^10.4.16",
    "eslint": "8.50.0",
    "eslint-config-next": "13.5.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.0.2",
    "msw": "^2.0.9",
    "next-router-mock": "^0.9.10",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "typescript": "5.2.2"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "**.{js,jsx,ts,tsx,json,css,md}": [
      "prettier --write --ignore-unknown"
    ]
  }
}
